<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<Tab text="cross" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Elyas.LssTestSheets.viewController.QualReportController">
	<content>
		<HBox>
			<children>
				<VBox HBox.hgrow="ALWAYS">
					<HBox.margin>
						<Insets right="5.0" />
					</HBox.margin>
					<children>
						<Label text="Clients" />
						<ListView fx:id="lstClients" onMouseClicked="#onClientSelect" />
					</children>
				</VBox>
				<VBox HBox.hgrow="ALWAYS">
					<children>
						<Label text="Notes" />
						<TextArea fx:id="txtNotes" VBox.vgrow="ALWAYS" />
					</children>
				</VBox>
			</children>
			<padding>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</padding>
		</HBox>
	</content>
</Tab>
