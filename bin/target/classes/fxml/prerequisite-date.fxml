<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.VBox?>
<?import org.controlsfx.control.textfield.CustomTextField?>

<TitledPane fx:id="pneMain" text="untitled" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.60" fx:controller="Elyas.LssTestSheets.viewController.PrerequisiteController">
  <content>
      <VBox>
         <children>
            <DatePicker fx:id="dteDate" promptText="Date" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </DatePicker>
            <CustomTextField fx:id="txtLocation" promptText="Location" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </CustomTextField>
         </children>
      </VBox>
  </content>
</TitledPane>
